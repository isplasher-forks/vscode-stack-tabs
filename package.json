{
    "name": "stack-tabs",
    "displayName": "stack-tabs",
    "description": "Keeps the most used tabs on the far left.",
    "version": "0.0.2",
    "publisher": "kylepaulsen",
    "author": {
        "name": "Kyle Paulsen"
    },
    "icon": "icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.11.0"
    },
    "repository": {
        "url": "https://github.com/kylepaulsen/vscode-stack-tabs.git",
        "type": "git"
    },
    "bugs": {
        "url": "https://github.com/kylepaulsen/vscode-stack-tabs/issues"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "stackTabs.stackTab",
            "title": "StackTabs: Move tab to the far left"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
